import java.util.Scanner;

Scanner sumOfOneThird
{
  public static void main(String[] args);
  {
    
    Scanner scan = new Scanner(System.in);
    
    int n, x;
    double sum;
    
    System.out.print("Enter a number: ")
    n = scan.nextInt();
    
    while( x <= n)
    {
      sum = sum + (1/x);
      x++;
    }
    System.out.print("The sum is: " + sum);
    
  }
}